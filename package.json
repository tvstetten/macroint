{
    "name": "macroint",
    "description": "Interpolate Macros in Strings, Object-Properties and Array-Elements.",
    "author": "Thomas von Stetten",
    "license": "MIT",
    "version_source": "www.semver.org",
    "version": "0.3.0",
    "main": "index.js",
    "types": "index.d.ts",
    "directories": {
        "examples": "examples"
    },
    "keywords": [
        "config-updater",
        "config",
        "env",
        "environment",
        "property",
        "configuration",
        "config-management"
    ],
    "readmeFilename": "README.md",
    "scripts": {
        "test": "mocha",
        "coverage": "c8 --reporter=text mocha",
        "ts-types": "npx -p typescript tsc *.js --declaration --allowJs --emitDeclarationOnly --outDir .",
        "doc": "yarn jsdoc index.js -d ./jsdoc/ --readme ./README.md",
        "md": "node ./jsdoc2md/build_readme.js"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/tvstetten/macroint.git"
    },
    "dependencies": {},
    "cspell": {
        "words": [
            "stetten",
            "macroint",
            "emptyarray",
            "macrokey",
            "repositorycallback",
            "modifiercallback",
            "macrosymbols",
            "olleH",
            "MAXCOUNT",
            "rawinclude"
        ]
    },
    "devDependencies": {
        "@types/node": "^18.11.9",
        "c8": "^7.12.0",
        "chai": "^4.3.7",
        "dmd-readable": "^1.2.4",
        "jsdoc": "^4.0.0",
        "jsdoc-to-markdown": "^8.0.0",
        "mocha": "^10.1.0"
    }
}